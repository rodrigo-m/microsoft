{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory Name",
			"defaultValue": "rmdf3"
		},
		"integrationRuntime_SelfHostedIntegrationRuntime": {
			"type": "string",
			"metadata": "Integration Runtime Name",
			"defaultValue": "SelfHostedIntegrationRuntime"
		},
		"rmadlsgen2_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'rmadlsgen2'"
		},
		"rmblob_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'rmblob'"
		},
		"rmadlsgen2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://rmadlsgen2.dfs.core.windows.net"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas Token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"integrationRuntime_SelfHostedIntegrationRuntime": {
						"value": "[parameters('integrationRuntime_SelfHostedIntegrationRuntime')]"
					},
					"rmadlsgen2_accountKey": {
						"value": "[parameters('rmadlsgen2_accountKey')]"
					},
					"rmblob_connectionString": {
						"value": "[parameters('rmblob_connectionString')]"
					},
					"rmadlsgen2_properties_typeProperties_url": {
						"value": "[parameters('rmadlsgen2_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}